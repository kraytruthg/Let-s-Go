<li class="pull-left dropdown">
  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
    <%= params[:date] ? params[:date] : "Select Date" %>
    <span class="caret"></span>
  </a>
  <ul class="dropdown-menu" role="menu">
    <li><%= link_to 'none', trip_path(trip) %></li>
    <% trip.start_date.upto(trip.end_date).each do |date| %>
      <li><%= link_to format_date(date), trip_path(@trip, date: date) %></li>
    <% end %>
  </ul>
</li>